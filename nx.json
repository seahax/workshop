{
  "tui": {
    "enabled": false
  },
  "targetDefaults": {
    "build": {
      "cache": true,
      "dependsOn": [
        "^build"
      ]
    },
    "test": {
      "dependsOn": [
        "build",
        "^test"
      ]
    },
    "start": {
      "dependsOn": [
        "build"
      ]
    },
    "docker.build": {
      "dependsOn": [
        "build"
      ]
    },
    "docker.start": {
      "dependsOn": [
        "docker.build"
      ]
    }
  }
}
