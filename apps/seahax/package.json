{
  "name": "app-seahax",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "run-all build.",
    "build.0-frontend": "vite build -c frontend/vite.config.ts",
    "build.1-frontend-types": "tsc -b -f frontend/tsconfig.json",
    "build.2-api": "cd api && GOOS=linux CGO_ENABLED=0 go build -o dist/api main.go",
    "clean": "rm -rf backend/dist frontend/dist",
    "docker": "run-all docker.",
    "docker.build": "./scripts/docker-build.sh",
    "docker.start": "./scripts/docker-start.sh",
    "lint": "eslint */src",
    "modernize": "cd api && go get -u && go mod tidy",
    "start": "cd api && go run main.go",
    "test": "run-all test.",
    "test.0-lint": "nx lint --max-warnings=0",
    "test.1-api": "cd api && go test -v -count=1"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.8.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource-variable/fredoka": "^5.2.10",
    "@fontsource/walter-turncoat": "^5.2.7",
    "@mui/material": "^7.3.4",
    "@seahax/glimmer": "workspace:*",
    "@sentry/react": "^10.22.0",
    "@tabler/icons-react": "^3.35.0",
    "notistack": "^3.0.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router": "^7.9.4"
  },
  "devDependencies": {
    "@mdx-js/rollup": "^3.1.1",
    "@mdx-js/typescript-plugin": "^0.1.2",
    "@seahax/eslint": "workspace:*",
    "@seahax/run-all": "workspace:*",
    "@seahax/vite-plugin-data": "workspace:*",
    "@seahax/vite-plugin-lib": "workspace:*",
    "@seahax/vite-plugin-prefetch": "workspace:*",
    "@sentry/vite-plugin": "^4.5.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "execa": "^9.6.0"
  }
}