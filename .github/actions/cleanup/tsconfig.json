{
  "compilerOptions": {
    "composite": true,
    "target": "ES2023",
    "module": "Node16",
    "moduleDetection": "force",
    "types": [ "node" ],
    "strict": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitOverride": true,
    "esModuleInterop": true,
    "noEmit": true
  }
}