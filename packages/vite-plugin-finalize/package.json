{
  "name": "@seahax/vite-plugin-finalize",
  "private": false,
  "version": "0.1.8",
  "description": "Run a task after the bundle is written.",
  "license": "Unlicense",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seahax/workshop.git",
    "directory": "packages/vite-plugin-finalize"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "pnpm clean && vite build && tsc -b src/tsconfig.json --force"
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "!dist/*.tsbuildinfo"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@seahax/vite-plugin-external": "workspace:*"
  },
  "dependencies": {
    "execa": "^9.5.2"
  },
  "peerDependencies": {
    "vite": "catalog:"
  }
}
