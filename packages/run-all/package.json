{
  "name": "@seahax/run-all",
  "version": "0.1.9",
  "private": false,
  "description": "Because life's too short to type 'npm run test:unit && npm run test:integration && npm run test:e2e' when you could just say 'run-all test'.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seahax/workshop.git",
    "directory": "packages/run-all"
  },
  "license": "Unlicense",
  "type": "module",
  "bin": "src/bin.ts",
  "files": [
    "src"
  ],
  "scripts": {
    "diff": "npm run build >/dev/null && npm diff 'dist/**.mjs' 'dist/**.cjs' 'dist/**.js' 'dist/**.d.ts'",
    "start": "node ./src/bin.ts",
    "test": "tsc -b -f tsconfig.build.json && eslint src"
  },
  "dependencies": {
    "@seahax/cli": "^0.1.0-prerelease.0",
    "execa": "^9.6.0"
  },
  "devDependencies": {
    "@seahax/eslint": "^0.2.27",
    "eslint": "^9.39.2"
  },
  "engines": {
    "node": ">=22.18.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
