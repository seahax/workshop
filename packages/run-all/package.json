{
  "name": "@seahax/run-all",
  "version": "0.1.8",
  "private": false,
  "description": "Because life's too short to type 'npm run test:unit && npm run test:integration && npm run test:e2e' when you could just say 'run-all test'.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seahax/workshop.git",
    "directory": "packages/run-all"
  },
  "license": "Unlicense",
  "type": "module",
  "bin": "bin.mjs",
  "files": [
    "bin.mjs",
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc -b -f tsconfig.build.json",
    "clean": "rm -rf dist",
    "diff": "npm run build >/dev/null && npm diff 'dist/**.mjs' 'dist/**.cjs' 'dist/**.js' 'dist/**.d.ts'",
    "test": "eslint src"
  },
  "dependencies": {
    "@seahax/args": "^0.6.1",
    "execa": "^9.6.0"
  },
  "devDependencies": {
    "@seahax/eslint": "^0.2.27",
    "@seahax/vite-plugin-lib": "^0.8.7",
    "eslint": "^9.39.2",
    "vite": "^7.3.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
