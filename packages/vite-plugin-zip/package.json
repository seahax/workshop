{
  "name": "@seahax/vite-plugin-zip",
  "version": "0.1.12",
  "private": false,
  "description": "Vite plugin that zips up the build output.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seahax/workshop.git",
    "directory": "packages/vite-plugin-zip"
  },
  "license": "Unlicense",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "!dist/*.tsbuildinfo"
  ],
  "scripts": {
    "build": "vite build && tsc -b -f src/tsconfig.json",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@types/archiver": "^7.0.0",
    "archiver": "^7.0.1",
    "fast-glob": "^3.3.3",
    "minimatch": "^10.1.1"
  },
  "devDependencies": {
    "@seahax/vite-plugin-lib": "workspace:*"
  },
  "peerDependencies": {
    "vite": ">=7.2.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
