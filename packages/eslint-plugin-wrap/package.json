{
  "name": "@seahax/eslint-plugin-wrap",
  "version": "0.6.10",
  "private": false,
  "description": "ESLint plugin with wrapping fixes for long lines.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seahax/workshop.git",
    "directory": "packages/eslint-plugin-wrap"
  },
  "license": "Unlicense",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc -b -f tsconfig.build.json",
    "clean": "rm -rf dist",
    "diff": "pnpm build >/dev/null && npm diff 'dist/**.mjs' 'dist/**.cjs' 'dist/**.js' 'dist/**.d.ts'",
    "prepublishOnly": "pnpm test && pnpm build",
    "test": "eslint src"
  },
  "dependencies": {
    "@types/eslint": "^9.6.1",
    "@typescript-eslint/utils": "^8.48.0"
  },
  "devDependencies": {
    "@seahax/eslint": "^0.2.27",
    "@seahax/vite-plugin-lib": "^0.8.7",
    "vite": "^7.3.1"
  },
  "peerDependencies": {
    "@typescript-eslint/parser": "^8.48.0",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
