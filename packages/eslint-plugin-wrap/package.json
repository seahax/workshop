{
  "name": "@seahax/eslint-plugin-wrap",
  "private": false,
  "version": "0.6.1",
  "description": "ESLint plugin with wrapping fixes for long lines.",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "plugin",
    "wrap",
    "fix",
    "array",
    "object",
    "import",
    "export",
    "ternary",
    "union",
    "function",
    "chain",
    "length",
    "indent",
    "format",
    "pretty",
    "prettier"
  ],
  "license": "Unlicense",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seahax/workshop.git",
    "directory": "packages/eslint-plugin-wrap"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "pnpm clean && vite build"
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "!dist/*.tsbuildinfo"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@seahax/vite-plugin-lib": "workspace:*"
  },
  "dependencies": {
    "@types/eslint": "catalog:",
    "@typescript-eslint/utils": "^8.20.0"
  },
  "peerDependencies": {
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "catalog:",
    "typescript": "catalog:"
  }
}
