{
  "name": "@seahax/eslint-progress",
  "version": "0.1.17",
  "private": false,
  "description": "ESLint plugin that prints filenames as they are linted.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seahax/workshop.git",
    "directory": "packages/eslint-progress"
  },
  "license": "Unlicense",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/plugin.d.ts",
      "import": "./src/plugin.mjs"
    }
  },
  "types": "src/plugin.d.ts",
  "files": [
    "src"
  ],
  "scripts": {
    "diff": "npm run build >/dev/null && npm diff 'dist/**.mjs' 'dist/**.cjs' 'dist/**.js' 'dist/**.d.ts'",
    "prepublishOnly": "npm test",
    "test": "eslint src"
  },
  "peerDependencies": {
    "@seahax/eslint": "^0.2.27",
    "eslint": "^9.39.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
