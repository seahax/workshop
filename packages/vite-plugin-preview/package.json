{
  "name": "@seahax/vite-plugin-preview",
  "version": "0.1.15",
  "private": false,
  "description": "Vite plugin that starts a preview server when the build command is run with the `--watch` option.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seahax/workshop.git",
    "directory": "packages/vite-plugin-preview"
  },
  "license": "Unlicense",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "!dist/*.tsbuildinfo"
  ],
  "scripts": {
    "build": "vite build && tsc -b -f tsconfig.json",
    "clean": "rm -rf dist",
    "demo": "pnpm build && vite build --watch -c demo/vite.config.ts"
  },
  "dependencies": {
    "@seahax/deep-copy": "workspace:^",
    "@seahax/semaphore": "workspace:^",
    "@types/ws": "^8.18.1",
    "escape-goat": "^4.0.0",
    "strip-ansi": "^7.1.2",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@seahax/vite-plugin-data": "workspace:*",
    "@seahax/vite-plugin-lib": "workspace:*"
  },
  "peerDependencies": {
    "vite": ">=7.1.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
